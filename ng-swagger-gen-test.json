{
  "$schema": "../../node_modules/ng-swagger-gen/ng-swagger-gen-schema.json",
  "output": "src/lib/api",
  "prefix": "Api",
  "ignoreUnusedModels": true,
  "minParamsForContainer": 2,
  "sortParams": "desc",
  "defaultTag": "Api",
  "removeStaleFiles": true,
  "modelIndex": true,
  "serviceIndex": true,
  "apiModule": true,
  "enumModule": true,
  "generateExamples": false,
  "camelCase": true,
  "generateFormBuilder": true,
  "includeTags": [
    "IoManagement",
    "Item",
    "ItemGroup",
    "ItemEndPoint",
    "ComplexItem",
    "ExchangeParameters",
    "SimulationItem",
    "SimulationItemGroup",
    "SimulationItemEndPoint"
  ]
}
